{
  "name": "vue-playground",
  "version": "1.0.0",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "dev": "vite",
    "build:dev": "yarn typecheck && vite build --mode dev",
    "build:stg": "yarn typecheck && vite build --mode stg",
    "build:prd": "yarn typecheck && vite build --mode prd",
    "preview": "vite preview",
    "lint:prettier": "prettier -c -u .",
    "lint:script": "eslint --ext .ts,.js,.vue .",
    "lint:style": "stylelint **/*.{css,vue} --ignore-path .gitignore",
    "lintfix": "yarn lint:prettier --write; yarn lint:script --fix; yarn lint:style --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ci": "vitest run --reporter=junit",
    "coverage": "vitest run --coverage",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "vue": "^3.2.37",
    "vue-router": "^4.1.3"
  },
  "devDependencies": {
    "@testing-library/vue": "^6.6.1",
    "@types/node": "^18.6.2",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vue/test-utils": "^2.0.2",
    "autoprefixer": "^10.4.7",
    "c8": "^7.12.0",
    "esbuild": "^0.14.51",
    "eslint": "^8.20.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-vue": "^9.3.0",
    "eslint-plugin-vue-scoped-css": "^2.2.0",
    "jsdom": "^20.0.0",
    "msw": "^0.47.4",
    "postcss": "^8.4.17",
    "postcss-html": "^1.5.0",
    "postcss-import": "^15.0.0",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "stylelint": "^14.13.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^28.0.0",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.4",
    "unplugin-auto-import": "^0.10.2",
    "unplugin-vue-components": "^0.21.2",
    "vite": "^3.0.4",
    "vite-plugin-pages": "^0.25.0",
    "vitest": "^0.19.1",
    "vue-eslint-parser": "^9.0.3",
    "vue-tsc": "^0.39.2"
  },
  "repository": {
    "url": "https://bitbucket.org/sonicmoov/vue3-starter-kit-ts"
  }
}
